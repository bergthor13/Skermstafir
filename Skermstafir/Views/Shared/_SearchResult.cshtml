<table class="table table-striped">
	<thead>
		<tr>
			<th class="min">Upp</th>
			<th>Titill</th>
			<th class="min">Útgáfuár</th>
			<th>Tungumál</th>
			<th>Stofnandi</th>
			<th class ="min">Hala</th>
			<th class="min">Niðurhöl</th>
		</tr>
	</thead>
	<tbody>
		@for (int i = 0; i < Model.modelList.Count; i++)
		{
			<tr class="search-result-row">
				<td align ="center">▲ @Model.modelList[i].votes</td>
				<td>@Model.modelList[i].name</td>
				<td align="center">@Model.modelList[i].yearCreated</td>
				<td>@Model.modelList[i].language</td>
				<td>@Model.modelList[i].user</td>
				<td align="center">▼</td>
				<td>@Model.modelList[i].downloads</td>
			</tr>
		}
	</tbody>

</table>